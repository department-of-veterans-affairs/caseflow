{
  "name": "caseflow-frontend",
  "private": true,
  "repository": "https://github.com/department-of-veterans-affairs/caseflow",
  "license": "none",
  "version": "0.0.1",
  "scripts": {
    "test": "yarn run test:karma && yarn run test:legacy",
    "test:legacy": "NODE_ENV=test mocha --recursive test/node/setup.js test/node/* --compilers jsx:babel-core/register js:babel-core/register",
    "test:karma": "NODE_ENV=test karma start",
    "test:karma:watch": "yarn run test:karma --no-single-run",
    "test:legacy:watch": "yarn run test:legacy --watch",
    "lint": "eslint . .eslintrc.js --no-ignore --ext .jsx --ext .js --max-warnings 0 --cache",
    "lint:fix": "yarn run lint --fix",
    "build": "webpack --config webpack.config.js",
    "build:test": "NODE_ENV=test yarn run build",
    "build:demo": "NODE_ENV=development yarn run build",
    "build:production": "NODE_ENV=production yarn run build",
    "dev:clear-build": "rm ../app/assets/webpack/* || true",
    "dev": "yarn run dev:clear-build && NODE_ENV=development yarn run build -w",
    "dev:hot": "yarn run dev:clear-build && NODE_ENV=development-hot node webpack-hot-server.js"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "@department-of-veterans-affairs/appeals-frontend-toolkit": "^2.1.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.5",
    "enzyme-adapter-react-16": "^1.1.1",
    "es5-shim": "^4.5.7",
    "expose-loader": "^0.7.3",
    "glamor": "^2.20.40",
    "history": "^4.5.1",
    "immutability-helper": "^2.4.0",
    "imports-loader": "^0.7.1",
    "jsdom": "9.8.3",
    "lodash": "^4.17.2",
    "mark.js": "^8.11.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "pdf-annotate.js": "^1.0.0",
    "pdfjs-dist": "1.6.414",
    "prop-types": "^15.6.0",
    "rc-collapse": "^1.7.6",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-highlight-words": "^0.8.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-on-rails": "^8.0.6",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.4",
    "react-textarea-autosize": "^5.1.0",
    "react-virtualized": "^9.10.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.0",
    "redux-perf-middleware": "^1.2.2",
    "redux-search": "^2.4.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "superagent": "^3.0.0",
    "superagent-no-cache": "^0.1.0",
    "uuid": "^3.0.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "@department-of-veterans-affairs/eslint-config-appeals": "^1.0.2",
    "babel-eslint": "^8.0.2",
    "babel-node-modules": "^0.0.1",
    "chai": "^3.5.0",
    "enzyme": "^3.3.0",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-mocha-snapshot": "^0.2.1",
    "karma-snapshot": "^0.5.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.1.2",
    "sinon": "^2.1.0",
    "webpack-dev-server": "^2.4.2"
  }
}
