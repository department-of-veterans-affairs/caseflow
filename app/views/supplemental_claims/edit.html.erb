<% content_for :full_page_content do %>
  <%= react_component("IntakeEdit", props: {
    userDisplayName: current_user.display_name,
    dropdownUrls: dropdown_urls,
    feedbackUrl: feedback_url,
    buildDate: build_date,
    formType: "supplemental_claim",
    veteran: {
      formName: supplemental_claim.veteran.name.formatted(:form),
      name: supplemental_claim.veteran.name,
      fileNumber: supplemental_claim.veteran_file_number
    },
    intake: {
      claimId: supplemental_claim.end_product_claim_id,
      receiptDate: supplemental_claim.receipt_date,
      issues: supplemental_claim.request_issues
    }
  }) %>
<% end %>
