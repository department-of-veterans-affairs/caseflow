<% content_for :full_page_content do %>
  <%= react_component("Queue", props: {
    userDisplayName: current_user.display_name,
    userId: current_user.id,
    userRole: (current_user.admin? && Rails.env.production?) ? "Attorney" : current_user.vacols_role,
    userCssId: current_user.css_id,
    userCanAccessQueue: can_access_queue?,
    dropdownUrls: dropdown_urls,
    feedbackUrl: feedback_url,
    buildDate: build_date,
    featureToggles: {
      phase_two: FeatureToggle.enabled?(:queue_phase_two, user: current_user),
      queue_case_search: FeatureToggle.enabled?(:queue_case_search, user: current_user),
      judge_assignment: FeatureToggle.enabled?(:judge_assignment, user: current_user)
    }
  }) %>
<% end %>
