<% content_for :full_page_content do %>
  <%= react_component("BaseContainer", props: {
    userDisplayName: current_user.display_name,
    dropdownUrls: dropdown_urls,
    page: "DecisionReviewer",
    feedbackUrl: feedback_url,
    featureToggles: {
      windowSlider: FeatureToggle.enabled?(:window_slider, user: current_user),
      queueWelcomeGate: FeatureToggle.enabled?(:queue_welcome_gate, user: current_user)
    },
    pdfWorker: asset_path('pdf.worker.js'),
    buildDate: build_date
  }) %>
<% end %>
