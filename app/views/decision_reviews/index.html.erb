<% content_for :full_page_content do %>
  <%= react_component("NonComp", props: {
    userDisplayName: current_user.display_name,
    dropdownUrls: dropdown_urls,
    feedbackUrl: feedback_url,
    buildDate: build_date,
    flash: flash,
    serverNonComp: {
      businessLine: business_line.name,
      businessLineUrl: business_line.url,
      inProgressTasks: apply_task_serializer(in_progress_tasks),
      completedTasks: apply_task_serializer(completed_tasks),
      featureToggles: {
        vet_ssn_instead_of_participant_id: FeatureToggle.enabled?(:vet_ssn_instead_of_participant_id, user: current_user)
      }
    }
  }) %>
<% end %>
